ex:person_jean_sammet rdf:type :Person ;
  :hasDisplayName "Jean Sammet" .

ex:person_unknown_speaker rdf:type :Person ;
  :hasDisplayName "Unknown speaker (interviewee)" .

ex:role_opponent rdf:type :Role ;
  :hasRoleName "opponent of picture clause" .

ex:role_proponent rdf:type :Role ;
  :hasRoleName "proponent of picture clause" .

ex:ref_jean_sammet_opponent rdf:type :PersonReference ;
  :referencesPerson ex:person_jean_sammet ;
